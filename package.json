{
	"name": "neph",
	"version": "1.0.0",
	"main": "dist/index.js",
	"private": true,
	"author": "@somelist",
	"license": "UNLICENSED",
	"type": "module",
	"packageManager": "pnpm@7.28.0",
	"imports": {
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#utils/*": "./dist/lib/util/*.js"
	},
	"scripts": {
		"build": "tsc -b src",
		"dev": "pnpm build && pnpm start",
		"postinstall": "husky install .github/husky && prisma generate",
		"lint": "eslint --fix --ext ts src",
		"start": "node --enable-source-maps dist/main.js",
		"format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,mts,json}\"",
		"update:i": "pnpm update --interactive",
		"update:l": "pnpm update --latest"
	},
	"dependencies": {
		"@kaname-png/plugin-subcommands-advanced": "^2.2.1",
		"@lavaclient/queue": "^2.1.1",
		"@lavaclient/spotify": "^3.1.0",
		"@prisma/client": "^4.11.0",
		"@sapphire/decorators": "^6.0.0",
		"@sapphire/discord.js-utilities": "^6.0.3",
		"@sapphire/framework": "^4.2.1",
		"@sapphire/pieces": "^3.6.0",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-subcommands": "^4.0.0",
		"@sapphire/type": "^2.3.0",
		"@sapphire/utilities": "^3.11.0",
		"@skyra/env-utilities": "^1.1.0",
		"colorette": "^2.0.19",
		"discord.js": "^14.7.1",
		"lavaclient": "^4.1.1",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@lavaclient/types": "^2.1.1",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/eslint": "^8.21.1",
		"@types/node": "^18.15.0",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"discord-api-types": "^0.37.35",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"prettier": "^2.8.4",
		"pretty-quick": "^3.1.3",
		"prisma": "^4.11.0",
		"typescript": "^4.9.5"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	}
}
